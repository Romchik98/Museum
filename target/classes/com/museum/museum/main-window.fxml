<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.museum.museum.controllers.MainWindowControl">
    <children>
        <TabPane fx:id="mainTab" prefHeight="411.0" prefWidth="675.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="courseTab" text="Collections">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="376.0" prefWidth="675.0">
                            <children>
                                <Label layoutX="134.0" layoutY="14.0" text="Collections" />
                                <ListView fx:id="collectionsList" layoutX="49.0" layoutY="52.0" onMouseClicked="#selectCollection" prefHeight="280.0" prefWidth="227.0" />
                                <ListView fx:id="exhibitsList" layoutX="399.0" layoutY="47.0" onMouseClicked="#selectExhibit" prefHeight="287.0" prefWidth="256.0" />
                                <Label layoutX="498.0" layoutY="14.0" text="Exhibits" />
                                <Button fx:id="editCourseButton" layoutX="76.0" layoutY="362.0" mnemonicParsing="false" onAction="#editCollection" text="Edit" />
                                <Button layoutX="113.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleteCollection" onMouseClicked="#deleteCollection" text="Delete" />
                                <Button fx:id="editFileButton" layoutX="552.0" layoutY="362.0" mnemonicParsing="false" onAction="#editExhibit" text="Edit" />
                                <Button layoutX="589.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleteExhibit" text="Delete" />
                                <Button fx:id="createCourseButton" layoutX="39.0" layoutY="362.0" mnemonicParsing="false" onAction="#createCollection" text="Add" />
                                <Button fx:id="createFileButton" layoutX="514.0" layoutY="362.0" mnemonicParsing="false" onAction="#createExhibit" text="Add" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Account settings">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TextField fx:id="loginName" layoutX="35.0" layoutY="94.0" prefHeight="25.0" prefWidth="250.0" />
                                <PasswordField fx:id="password" layoutX="35.0" layoutY="146.0" prefHeight="25.0" prefWidth="250.0" />
                                <TextField fx:id="name" layoutX="35.0" layoutY="198.0" prefHeight="25.0" prefWidth="250.0" />
                                <TextField fx:id="surname" layoutX="35.0" layoutY="250.0" prefHeight="25.0" prefWidth="250.0" />
                                <Label layoutX="35.0" layoutY="181.0" text="Name:" />
                                <Label layoutX="35.0" layoutY="129.0" text="Password:" />
                                <Label layoutX="35.0" layoutY="77.0" text="Login name:" />
                                <Label layoutX="35.0" layoutY="233.0" text="Surname:" />
                                <Button layoutX="508.0" layoutY="278.0" mnemonicParsing="false" onAction="#updateUser" prefHeight="44.0" prefWidth="129.0" text="Update" textFill="RED">
                                    <font>
                                        <Font size="24.0" />
                                    </font>
                                </Button>
                                <Label layoutX="373.0" layoutY="154.0" text="Enter account new details">
                                    <font>
                                        <Font size="24.0" />
                                    </font>
                                </Label>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Users">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <ListView fx:id="userListAdmin" layoutX="40.0" layoutY="40.0" onMouseClicked="#selectAdminUser" prefHeight="324.0" prefWidth="200.0" />
                                <Button fx:id="resetUserButton" layoutX="356.0" layoutY="249.0" mnemonicParsing="false" onAction="#resetUser" prefHeight="43.0" prefWidth="158.0" text="Reset">
                                    <font>
                                        <Font size="18.0" />
                                    </font>
                                </Button>
                                <Label layoutX="118.0" layoutY="23.0" text="All users" />
                                <Button fx:id="deleteUserButton" layoutX="356.0" layoutY="292.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="43.0" prefWidth="158.0" text="Delete" textFill="RED">
                                    <font>
                                        <Font size="21.0" />
                                    </font>
                                </Button>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
