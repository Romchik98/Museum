<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="322.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.museum.museum.controllers.SignUpControl">
    <children>
      <Label layoutX="101.0" layoutY="14.0" text="Login information:" />
        <Label layoutX="30.0" layoutY="31.0" text="Login:" />
        <TextField fx:id="loginName" layoutX="30.0" layoutY="48.0" prefHeight="25.0" prefWidth="245.0" />
        <Label layoutX="30.0" layoutY="73.0" text="Password:" />
        <PasswordField fx:id="password" layoutX="30.0" layoutY="90.0" prefHeight="25.0" prefWidth="245.0" />
      <Label layoutX="98.0" layoutY="127.0" text="Contact information:" />
      <Label layoutX="30.0" layoutY="144.0" text="First name:" />
      <TextField fx:id="name" layoutX="30.0" layoutY="161.0" prefHeight="25.0" prefWidth="245.0" />
      <Label layoutX="30.0" layoutY="186.0" text="Last name:" />
      <TextField fx:id="surname" layoutX="30.0" layoutY="203.0" prefHeight="25.0" prefWidth="245.0" />
        <Button layoutX="166.0" layoutY="278.0" mnemonicParsing="false" onAction="#startSignUp" prefHeight="25.0" prefWidth="109.0" text="Sign up" />
      <Button layoutX="31.0" layoutY="278.0" mnemonicParsing="false" onAction="#returnToLoginPage" text="Atgal" />
    </children>
</AnchorPane>
